{
  "name": "favicons",
  "version": "6.2.2",
  "description": "Favicon generator for Node.js",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/itgalaxy/favicons.git"
  },
  "scripts": {
    "fmt": "prettier --write .",
    "fmt-check": "prettier --check .",
    "lint": "eslint . --ignore-path .gitignore",
    "fix": "npm run lint -- --fix && npm run fmt",
    "pretest": "npm run lint && npm run fmt-check",
    "test-only": "ava",
    "test": "npm run test-only",
    "coverage": "nyc npm test",
    "prepare": "gulp"
  },
  "ava": {
    "require": [
      "@babel/register"
    ],
    "files": [
      "test/**/*.test.js",
      "test/*.test.js"
    ],
    "timeout": "2m"
  },
  "keywords": [
    "favicon",
    "ico",
    "generator",
    "node",
    "realfavicongenerator",
    "gulpfriendly"
  ],
  "author": "Hayden Bleasel <haydenbleasel@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itgalaxy/favicons/issues"
  },
  "homepage": "https://github.com/itgalaxy/favicons",
  "engines": {
    "node": ">=10.13.0"
  },
  "dependencies": {
    "clone": "^2.1.2",
    "colors": "^1.4.0",
    "image-size": "^1.0.1",
    "jimp": "^0.16.1",
    "jsontoxml": "^1.0.1",
    "lodash.defaultsdeep": "^4.6.1",
    "require-directory": "^2.1.1",
    "sharp": "^0.30.2",
    "through2": "^4.0.2",
    "tinycolor2": "^1.4.2",
    "to-ico": "^1.1.5",
    "vinyl": "^2.2.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "ava": "^4.1.0",
    "babel-plugin-istanbul": "^6.1.1",
    "concordance-comparator": "^0.1.0",
    "del": "^6.0.0",
    "dir-compare": "^4.0.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fibers": "^5.0.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-eslint": "^6.0.0",
    "icojs": "^0.16.1",
    "nyc": "^15.1.0",
    "pixelmatch": "^5.2.1",
    "prettier": "^2.5.1",
    "resemblejs": "^4.1.0"
  }
}
